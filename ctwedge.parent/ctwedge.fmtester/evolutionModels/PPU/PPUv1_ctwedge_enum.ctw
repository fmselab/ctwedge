Model MPPUv1 Parameters :
PPU : Boolean
Crane : Boolean
SRouting : Boolean
Slide : { Straight Conveyor NONE }
Conveyor : { SSortation DSortation NONE }
Workpiece : Boolean Constraints :
# PPU == TRUE #
# Crane == TRUE <=> PPU == TRUE #
# SRouting == TRUE <=> Crane == TRUE #
# Slide != NONE <=> PPU == TRUE #
# Conveyor != NONE <=> Slide == Conveyor #
# Workpiece == TRUE <=> PPU == TRUE #