	# ( p2!=v3 OR p1!=v4) #
	# ( p8!=v1 OR p6!=true OR p9!=v2 OR p7!=v1) #
	# ( p2!=v2 OR p1!=v3) #
	# ( p8!=v1 OR p6!=true OR p9!=v3 OR p7!=v1) #
	# ( p2!=v1 OR p1!=v4) #
	# ( p2!=v3 OR p1!=v1) #
	# ( p2!=v2 OR p1!=v5) #
	# ( p8!=v1 OR p7!=v3 OR p6!=true OR p9!=v1) #
	# ( p8!=v1 OR p6!=true OR p7!=v4 OR p9!=v1) #
	# ( p2!=v1 OR p1!=v1) #
	# ( p8!=v1 OR p7!=v1 OR p9!=v1) #
	# ( p1!=v5 OR p2!=v4) #
	# ( p8!=v1 OR p6!=true OR p7!=v5 OR p9!=v1) #
	# ( p9!=v4 OR p8!=v1 OR p6!=true OR p7!=v1) #
	# ( p8!=v1 OR p6!=true OR p9!=v1 OR p7!=v6) #
	# ( p8!=v1 OR p6!=true OR p7!=v2 OR p9!=v1) #
	# ( p8!=v2 OR p6!=true OR p9!=v1 OR p7!=v1) #

