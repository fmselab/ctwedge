<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
      <extension
      	id="CSV"
         name="CSVExporter"
         point="ctwedge.ui.ctwedgeExporters">
      <TestSuiteExporter
            ExporterName="CSV"
            FileExtension="*.csv"
            ITSPrototype="ctwedge.generator.exporter.CSVExporter">
      </TestSuiteExporter>
   </extension>
   
   <extension
   		id="XLS"
         name="ExcelExporter (.xls)"
         point="ctwedge.ui.ctwedgeExporters">
      <TestSuiteExporter
            ExporterName="XLS"
            FileExtension="*.xls"
            ITSPrototype="ctwedge.generator.exporter.XlsExporter">
      </TestSuiteExporter>
   </extension>

</plugin>
